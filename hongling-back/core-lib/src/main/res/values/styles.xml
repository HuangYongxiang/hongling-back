<?xml version="1.0" encoding="utf-8"?>
<resources >
    <!--加载提示框-->
    <style name="core_loading_dialog_style" parent="android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimAmount">0.4</item>
    </style>
    <!--加载圆形转圈图片-->
    <style name="core_circleProgressBarStyle">
        <item name="android:indeterminateDrawable">@drawable/core_rotate_loading_icon</item>
        <item name="android:indeterminateDuration">1500</item>
    </style>
</resources>