<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="photo_Rotate3dAnimation">
        <attr name="photo_rollType" format="enum">
            <enum name="x" value="0"/>
            <enum name="y" value="1"/>
            <enum name="z" value="2"/>
        </attr>
        <attr name="photo_fromDeg" format="float" />
        <attr name="photo_toDeg" format="float" />
        <attr name="photo_pivotX" format="fraction"/>
        <attr name="photo_pivotY" format="fraction" />
    </declare-styleable>

