<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/survey_white"/>
            <corners android:topLeftRadius="4dp" android:topRightRadius="4dp"/>
            <stroke
                android:width="0.4dp"
                android:color="@color/survey_gray_btn_bg_color"/>
        </shape>
    </item>
    <item android:top="0.4dp" android:left="0.4dp" android:right="0.4dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/survey_white"/>
            <corners android:topLeftRadius="4dp" android:topRightRadius="4dp"/>
            <stroke
                android:width="0.4dp"
                android:color="@color/survey_white"/>
        </shape>
    </item>

</layer-list>